<!-- ko if: initialized() && $data.elements.hasOwnProperty('product-variants-listing') -->

<div class="pdp-variant-options-listing" data-bind="onRender: $parent.elements['product-variants-listing'].autoSelect()">
    <!-- <h5 class="pdp-variant-options-listing--list-count" data-bind="text: '(' + $parent.elements[$elementInstance.type].allItems().length + ' items)' "></h5>
 -->

    <!-- ko foreach: $parent.elements[$elementInstance.type].infinateScrollList -->
    <form data-bind="attr:{id: 'form-' + $data.repositoryId()}">

    <div class="row-fluid"  >

        <!--  -->

        <div class="col-sm-3">
            <img class="pdp-variant-options-listing--image" data-bind="ccResizeImage: {
    source: '/file/products/'+$data.repositoryId()+'.jpg',
    size:'300,320',
    alt: 'Antique Wood Chair',
    errorSrc:'images/noImage.png',
    errorAlt:'No Image Found'}">
        </div>
        <div class="col-sm-6">
            <!-- ko if: $data.displayName != null && typeof $data.displayName != 'undefined' -->
                <div><h4 class="pdp-variant-options-listing-header" data-bind="text:$data.displayName()"></h4></div>
            <!-- /ko -->
                <div>
                    <!-- ko if: $data.hc_size != null && typeof $data.hc_size != 'undefined' -->
                        <div><b>Size:</b> <span data-bind="text:$data.hc_size()"></span></div>
                    <!-- /ko -->
                    <!-- ko if: $data.hc_colour != null && typeof $data.hc_colour != 'undefined' -->
                        <div><b>Color:</b> <span data-bind="text:$data.hc_colour()"></span></div>
                    <!-- /ko -->
                    <!-- ko if: $data.hc_piece != null && typeof $data.hc_piece != 'undefined' -->
                        <div><b>Pieces:</b> <span data-bind="text:$data.hc_piece()"></span></div>
                    <!-- /ko -->
                    <!-- ko if: $data.hc_orderCode != null && typeof $data.hc_orderCode != 'undefined' -->
                        <div><b>Product code:</b> <span data-bind="text:$data.hc_orderCode()"></span></div>
                    <!-- /ko -->
                </div>

                            <!-- ko if: $data.hc_skuLongDescription != null && typeof $data.hc_skuLongDescription != 'undefined' -->
                <div>
                    <div class="pdp-variant-options-listing-inc">Your set includes:</div>
                    <div class="pdp-variant-options-listing-inc-text" data-bind="html:$data.hc_skuLongDescription()">
                    </div>
                </div>
                            <!-- /ko -->

   <!-- ko if: $parent.elements['product-variants-listing'].isAppropriateProduct -->
            <br>
            <div class="DeliverRestrictions">
                <div class="DeliverRestrictionsHead">
                    This is a special delivery item:
                </div>
                <div data-bind="attr:{id: 'SKUitem-' + $data.repositoryId()}, onRender: $parent.elements['product-variants-listing'].GetCustomMessage($data.repositoryId())">
                    
                </div>
            </div> 
    <!-- /ko -->




        </div>
        <div class="col-sm-3">
            <div data-bind="attr:{id:'termsList-' + $index()}">

                <div class="product-details-credit-options clearfix">
                    <div class="control-group col-xs-12">
                        <label class="control-label product-details-credit-options-label">Choose Payment</label>
                    </div>
                    <div class="col-xs-12">
                        <ul class="product-details-credit-options--list clearfix" data-bind="foreach: $parent.elements[$elementInstance.type].getTermsListBySku($data.repositoryId())">
                            <li class="col-xs-6 col-sm-4">
                                <input type="radio" data-bind="click: $parentContext.$parent.elements[$elementInstance.type].onPaymentOptionSelected, attr:{'value':$data.id, 'name':$parent.repositoryId() + 'creditPaymentSelections', 'id': $parent.repositoryId()+'-creditOpt-'+ $index()}">
                                <label data-bind="'text':$data.label, attr:{'for': $parent.repositoryId()+'-creditOpt-'+ $index()}"></label>
                            </li>
                        </ul>
                    </div>



                    <div  class="product-details-credit-options--list-button" data-bind="attr:{id:'addToCart-' + $index()}">
                            <!-- ko if: $data.quantity() > 0 -->
                        <button disabled data-bind="attr:$data.variantObj, event:{click: $parent.elements[$elementInstance.type].handleAddToCart.bind($parent.elements[$elementInstance.type], $parent ) }" class="cc-button-primary" aria-disabled="true" tabindex="0">
                                <span id="cc-prodDetails-addToCart">
                                        <i class="fa fa-shopping-basket"></i>
                                    Add to Basket</span>
                            </button>
                                <!-- /ko -->
                                <!-- ko if: $data.quantity() == 0 -->
                        <a disabled class="cc-button-primary" href="#" onclick="return false;" aria-disabled="true" tabindex="0">
                                <span id="cc-prodDetails-addToCart">
                                    Out Of Stock</span>
                                </a>
                                <!-- /ko -->
                    </div>



<!-- ko if: $parents[0].availabilityDate() -->
