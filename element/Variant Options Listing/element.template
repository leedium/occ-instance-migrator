  <div class="row">
    <div class="col-md-6">

      <div style="padding: 10px">
        <div style="color: #ed2282; font-weight: bold; font-size:16px;">
          Reasons to buy
        </div>
        <div style="padding: 0px 10px" data-bind="html: $data.product().longDescription"></div>
      </div>
