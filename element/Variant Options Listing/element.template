    <!-- /ko -->



    <div data-bind="">
            <div >
                <div class="custom-wishlist" >
                    <button id="cc-prodDetailsAddToSpaceSelector" data-backdrop="static" data-bind="attr:$data.variantObj, disabled: {condition: $parent.elements['product-variants-listing'].customDisableAddToSpace}, click: $parent.elements['product-variants-listing'].customIsAddToSpaceClicked() == false && $parent.elements['product-variants-listing'].addToSpaceClick.bind($parent.elements[$elementInstance.type], $parent )" class="btn cc-button-secondary">
                        <!-- ko ifnot: $parent.elements['product-variants-listing'].customIsAddToSpaceClicked -->
                        <span id="CC-prodDetails-addToSpaceSelector" >Save for later</span>
                        <!-- /ko -->
                        <!-- ko if: $parent.elements['product-variants-listing'].customIsAddToSpaceClicked -->
                        <span id="CC-prodDetails-addedToSpaceSelector" class="Selected" >Saved for later</span>
                        <!-- /ko -->
                    </button>
        
                    <!-- k o i  f: $parent.user().loggedIn() - ->
                        <button id="cc-prodDetailsAddToSpaceDropdown" class="btn cc-button-secondary dropdown-toggle" data-backdrop="static" data-toggle="dropdown" 
                            data-bind="disabled: $parent.elements['product-variants-listing'].customDisableAddToSpace, click: $parent.elements['product-variants-listing'].openAddToWishlistDropdownSelector.bind($parent)">
                            <span class="caret"></span>
                            <span class="sr-only">Save for later</span>
                        </button>
